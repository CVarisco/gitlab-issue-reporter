{
  "manifest_version": 2,
  "name": "Gitlab issue reporter",
  "description": "Chrome extension to report issue on your own gitlab prject",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "optional_permissions": [
    "http://*/",
    "https://*/"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  }
}